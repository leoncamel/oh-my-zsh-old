alias splayerx='open -a /Applications/SPlayerX.app'
alias mplayerx='open -a /Applications/MPlayerX.app'

compdef '_files -g "*.(#i)(asf|asx|avi|flac|flv|m1v|m2p|m2v|mjpg|mka|mkv|mov|mp3|mp4|mpe|mpeg|mpg|ogg|ogm|qt|rm|vob|wav|wma|wmv)(-.)"' splayerx

